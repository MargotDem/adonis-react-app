{
  "name": "adonis-react-app",
  "version": "2.0.0",
  "adonis-version": "4.1.0",
  "description": "An application boilerplate for Adonisjs with a React frontend",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "test": "node ace test",
    "dev": "concurrently --kill-others-on-fail \"adonis serve --dev\" \"npm run development\"",
    "development": "cross-env NODE_ENV=development node_modules/.bin/webpack --watch --hide-modules --progress"
  },
  "keywords": [
    "adonisjs",
    "adonis-app",
    "adonis-react"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/session": "^1.0.27",
    "@adonisjs/shield": "^1.0.8",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass-loader": "^8.0.0",
    "webpack": "^4.41.5"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.10"
  },
  "autoload": {
    "App": "./app"
  }
}
